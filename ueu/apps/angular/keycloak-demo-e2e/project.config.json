{
    "angular": {
        "keycloak-demo-e2e": {
            "root": "apps/keycloak-demo-e2e",
            "sourceRoot": "apps/keycloak-demo-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/keycloak-demo-e2e/cypress.json",
                        "tsConfig": "apps/keycloak-demo-e2e/tsconfig.e2e.json",
                        "devServerTarget": "keycloak-demo:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "keycloak-demo:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["apps/keycloak-demo-e2e/tsconfig.e2e.json"],
                        "exclude": ["**/node_modules/**", "!apps/keycloak-demo-e2e/**"]
                    }
                }
            }
        }
    },
    "nx": {
        "keycloak-demo-e2e": {
            "tags": [],
            "implicitDependencies": ["keycloak-demo"]
        }
    },
    "tsconfig": {}
}
